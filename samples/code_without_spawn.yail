function power(Int n, Int k):Int{	
	if(k > 0){
		return n * power(n, k - 1);
	}
	else{
		return 1;
	}
}

function map_helper(Int[] arr, Int index, Function f):Int[]{
	if(index < arr.length){
		Int result = f(arr[index], arr[index]);
		Int[] list = map_helper(arr, index + 1, f);
		return [result] + list;
	}
	else{
		return [];
	}
}

function printArray(Int[] arr):Nothing{
	if(arr.length > 0){
		print(arr[0], "\n");
		printArray(arr[1:]);
	}
	else{
		newline();
	}
}

