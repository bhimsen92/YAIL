function move(Int n, Int from, Int to, Int via):Nothing
{
	if( n == 1 ){
		print("move disk 1 from peg: ");
		print(from);
		print(to);
		#newline();
	}
	else{
		move(n-1, from, via, to);
		print("move disk: ");
		print(n);
		print(from);
		print(to);
		#newline();
		move(n-1, via, to, from);
	}
}

move(3,1,2,3);
