function multi( Int a, Int b ):Int
{
    return a * b;
}

function fact( Int n ): Int
{
    if( n == 0 )
        return 1;
    else
        return n * fact( n - 1 );
}

function firstNSum( Int n ): Int
{
    if( n == 0 )
        return 0;
    else
        return n + firstNSum( n - 1 );
}

function circleArea( Double radius ): Double
{
    Double PI = 3.14285;
    return PI * radius * radius;
}

print( "Area of circle with radius 5 is:" );
print(circleArea( 5.0 ));
